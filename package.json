{
  "name": "openapi-mock-gen",
  "version": "1.0.0-alpha.0",
  "description": "Generate mock definitions with random fake data based on OpenAPI spec.",
  "keywords": [
    "openapi",
    "openapi-mock",
    "msw",
    "msw-auto-mock",
    "mock",
    "faker.js"
  ],
  "author": "noah",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "bin": {
    "openapi-mock-gen": "./dist/index.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:init": "npx tsx lib/cli.ts init",
    "dev": "npx tsx lib/cli.ts",
    "dev:msw": "npx tsx lib/cli.ts msw",
    "declaration:emit": "tsc -b tsconfig.build.json",
    "build": "rollup -c",
    "build:analyze": "ANALYZE=true rollup -c",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.8.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-swc": "^0.4.0",
    "@types/jest": "^29.5.14",
    "@types/lodash.merge": "^4.6.9",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "memfs": "^4.17.2",
    "msw": "^2.10.2",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "rollup": "^4.34.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-jest": "^29.2.5",
    "ts-jest-mock-import-meta": "^1.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asde29873012549/openapi-mock-gen.git"
  },
  "bugs": {
    "url": "https://github.com/asde29873012549/openapi-mock-gen/issues"
  },
  "homepage": "https://github.com/asde29873012549/openapi-mock-gen#readme",
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "cac": "^6.7.14",
    "enquirer": "^2.4.1",
    "jiti": "^2.4.2",
    "lodash.merge": "^4.6.2",
    "openapi-typescript": "^7.8.0",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1"
  },
  "peerDependencies": {
    "@faker-js/faker": "^8.0.0",
    "prettier": "^3.0.0"
  }
}

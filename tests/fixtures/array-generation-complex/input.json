{
  "openapi": "3.0.0",
  "info": {
    "title": "Complex Array Generation Test API",
    "version": "1.0.0"
  },
  "paths": {
    "/collections": {
      "get": {
        "summary": "Get collections with complex arrays",
        "responses": {
          "200": {
            "description": "Collections with various array types",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Collections"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Collections": {
        "type": "object",
        "properties": {
          "simpleStringArray": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "numberArray": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "booleanArray": {
            "type": "array",
            "items": {
              "type": "boolean"
            }
          },
          "enumArray": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["option1", "option2", "option3"]
            }
          },
          "objectArray": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string",
                  "format": "email"
                }
              }
            }
          },
          "nestedArray": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "mixedArray": {
            "type": "array",
            "items": {
              "oneOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                },
                {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "value": {
                      "type": "string"
                    }
                  }
                }
              ]
            }
          },
          "complexNestedArray": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "category": {
                  "type": "string"
                },
                "items": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "format": "uuid"
                      },
                      "metadata": {
                        "type": "object",
                        "properties": {
                          "tags": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            }
                          },
                          "scores": {
                            "type": "array",
                            "items": {
                              "type": "number"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "arrayWithHeuristicKeys": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "user_id": {
                  "type": "string"
                },
                "created_at": {
                  "type": "string"
                },
                "email_address": {
                  "type": "string"
                },
                "phone_number": {
                  "type": "string"
                },
                "total_amount": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    }
  }
} 